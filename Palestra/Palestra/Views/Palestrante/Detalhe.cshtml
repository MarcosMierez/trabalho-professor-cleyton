@model Palestra.Models.Palestrante

<div class="row">
    <div class="col-sm-6 col-md-4">
        <div class="">
            <img src="~/Content/Uploads/@Model.Foto" class="img-rounded"height="200" width="200">
            <div class="caption">
                <p> <strong>Nome  :</strong> @Model.Nome</p>
                <p><strong>Twitter :</strong> @Model.Twitter</p>
                <p><strong>Biografia :</strong>
                @Model.Bio
                </p>
                <p><a href="@Url.Action("Editar", "Palestrante", new { id=@Model.ID})" class="btn btn-info" role="button">Editar</a> <a href="@Url.Action("Delete", "Palestrante", new { id=@Model.ID})" class="btn btn-danger" role="button">Excuir</a></p>
            </div>
        </div>
    </div>
</div>
